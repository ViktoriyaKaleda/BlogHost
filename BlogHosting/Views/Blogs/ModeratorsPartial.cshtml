@model IEnumerable<BlogModerator>
<div class="container">
    @if (Model.Count() == 0)
    {
        <p id="noModerators">No moderators</p>
    }
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <label class="moderatorUsername">@item.Moderator.UserName</label>
                </td>
                <td>
                    <a class="deleteModerator" data-username="@item.Moderator.UserName" ><i class="fas fa-times"></i></a>
                </td>
            </tr>
        }
    </table>
</div>
